
#editor {
    padding: 16px 24px; 
    border: 1px solid #D6D6D6; 
    border-radius: 4px;
    text-align: left;
}

#editor > p {
    text-align: left;
    padding: 0;
    margin: 0;
}
#img {display: none;}
.editor-menu{
    text-align: center;
    background-color: #f5f5f5;
    border: 1px solid #ddd;
    padding: 0.5% 0% 0.5% 0%;
}
.editor_btn{padding: 1px 3px 1px 3px;}