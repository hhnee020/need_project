table {
    width: 80%;
    border: none;
}
th{
    text-align: center;
}

td{
    padding-left: 5%;
    padding-right: 5%;
    text-align: left;
    padding-top: 2.5%;
    padding-bottom: 2.5%;
}
#header_whiteSpace{
    margin-top: 10%;
}
.td{
    font-size: 1.2em;
    font-weight: 700;
}
.content{
    height: 500px;
    vertical-align: top;
    text-align: left;
}
#backList{
    text-align: left;
    margin: 0 10% 1% 10%;
}
#question_table{
    width: 80%;
}
#reply{
    vertical-align: top;
    padding-top: 5%;
    font-size: 1.3em;
    color: #04b4f6;
}
.reply_colum{
    border: none;
}
#reply_table{
    width: 80%;
    margin-top: 5%;
    background-color: rgb(243, 243, 243);
}
#reply_content{
    padding-top: 5%;
}